<tiles-definitions>
    <!-- 模块:questionnaire 业务对像:AnswerRecord 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- AnswerRecord 管理 首页(列表页) -->
	<definition name="questionnaire.answerRecord.manager" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.answerRecord.center"></put-attribute>
	</definition>
	<definition name="questionnaire.answerRecord.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/answerRecord/answerRecord_center.jsp"></put-attribute>
	</definition>
	
	<!-- AnswerRecord 新增,编辑页面 -->
	<definition name="questionnaire.answerRecord.update" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.answerRecord.update.center"></put-attribute>
	</definition>
	<definition name="questionnaire.answerRecord.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/answerRecord/answerRecord_update.jsp"></put-attribute>
	</definition>
	
	<!-- AnswerRecord 明细页 -->
	<definition name="questionnaire.answerRecord.detail" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.answerRecord.detail.center"></put-attribute>
	</definition>
	<definition name="questionnaire.answerRecord.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/answerRecord/answerRecord_detail.jsp"></put-attribute>
	</definition>
	
