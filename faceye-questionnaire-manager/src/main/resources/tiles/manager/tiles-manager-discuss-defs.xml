<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:questionnaire 业务对像:Discuss 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Discuss 管理 首页(列表页) -->
	<definition name="questionnaire.discuss.manager" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.discuss.center"></put-attribute>
	</definition>
	<definition name="questionnaire.discuss.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/discuss/discuss_center.jsp"></put-attribute>
	</definition>
	
	<!-- Discuss 新增,编辑页面 -->
	<definition name="questionnaire.discuss.update" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.discuss.update.center"></put-attribute>
	</definition>
	<definition name="questionnaire.discuss.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/discuss/discuss_update.jsp"></put-attribute>
	</definition>
	
	<!-- Discuss 明细页 -->
	<definition name="questionnaire.discuss.detail" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.discuss.detail.center"></put-attribute>
	</definition>
	<definition name="questionnaire.discuss.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/discuss/discuss_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
