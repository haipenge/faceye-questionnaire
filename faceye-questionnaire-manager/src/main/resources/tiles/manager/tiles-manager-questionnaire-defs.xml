<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:questionnaire 业务对像:Questionnaire 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Questionnaire 管理 首页(列表页) -->
	<definition name="questionnaire.questionnaire.manager" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.questionnaire.center"></put-attribute>
	</definition>
	<definition name="questionnaire.questionnaire.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/questionnaire/questionnaire_center.jsp"></put-attribute>
	</definition>
	
	<!-- Questionnaire 新增,编辑页面 -->
	<definition name="questionnaire.questionnaire.update" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.questionnaire.update.center"></put-attribute>
	</definition>
	<definition name="questionnaire.questionnaire.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/questionnaire/questionnaire_update.jsp"></put-attribute>
	</definition>
	
	<!-- Questionnaire 明细页 -->
	<definition name="questionnaire.questionnaire.detail" extends="default.manager">
	    <put-attribute name="default-center" value="questionnaire.questionnaire.detail.center"></put-attribute>
	</definition>
	<definition name="questionnaire.questionnaire.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/questionnaire/questionnaire/questionnaire_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
