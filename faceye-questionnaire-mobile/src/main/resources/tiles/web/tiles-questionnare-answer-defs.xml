    <!-- Answer 管理 首页(列表页) -->
	<definition name="questionnaire.answer.manager" extends="questionnaire.empty">
	    <put-attribute name="default-center" value="/component/questionnaire/answer/answer_center.jsp"></put-attribute>
	</definition>
	
	
	<!-- Answer 新增,编辑页面 -->
	<definition name="questionnaire.answer.update" extends="questionnaire.empty">
