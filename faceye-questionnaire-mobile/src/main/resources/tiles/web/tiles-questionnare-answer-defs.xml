	</definition>
	
	
	<!-- Answer 明细页 -->
	<definition name="questionnaire.answer.detail" extends="questionnaire.empty">
	    <put-attribute name="default-center" value="/component/questionnaire/answer/answer_detail.jsp"></put-attribute>
	</definition>
	
	
